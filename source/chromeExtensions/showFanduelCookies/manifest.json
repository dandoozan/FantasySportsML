{
    "name": "DF Cookie",
    "version": "0.1",
    "description": "",
    "permissions": [
        "tabs",
        "cookies",
        "*://*/*"
    ],
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_title": "DF Cookie",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google.com; object-src 'self'",
    "manifest_version": 2
}